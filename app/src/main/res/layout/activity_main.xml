<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView10"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:background="@drawable/textborder"
        android:gravity="center"
        android:text="재료 총합"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/SumIngredientText"
        app:layout_constraintStart_toEndOf="@+id/textView4" />

    <TextView
        android:id="@+id/FatigueElixirText"
        android:layout_width="100dp"
        android:layout_height="200dp"
        android:layout_marginBottom="400dp"
        android:background="@drawable/textborder"
        android:text="TextView"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/GoldCubeText"
        android:layout_width="100dp"
        android:layout_height="200dp"
        android:layout_marginBottom="400dp"

        android:background="@drawable/textborder"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/FatigueElixirText"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/SangSumText"
        android:layout_width="100dp"
        android:layout_height="200dp"

        android:layout_marginBottom="400dp"
        android:background="@drawable/textborder"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/GoldCubeText"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/AdventurerText"
        android:layout_width="100dp"
        android:layout_height="200dp"

        android:background="@drawable/textborder"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/magicCrystalText"
        android:layout_width="100dp"
        android:layout_height="200dp"

        android:background="@drawable/textborder"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/AdventurerText"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/noColorMagicText"
        android:layout_width="100dp"
        android:layout_height="200dp"

        android:background="@drawable/textborder"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/magicCrystalText"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="326dp"
        android:gravity="center"
        android:textStyle="bold"
        android:text="피로 회복 영약"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="황금 큐브 조각"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/GoldCubeText"
        app:layout_constraintStart_toEndOf="@+id/textView" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="생명의 숨결"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/SangSumText"
        app:layout_constraintStart_toEndOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:gravity="center"
        android:text="노련한 모험가"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/FatigueElixirText" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:gravity="center"
        android:text="마력 결정"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/GoldCubeText" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:gravity="center"
        android:text="무색 마력의 산물"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/SangSumText" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:background="@drawable/textborder"
        android:gravity="center"
        android:text="최저가의 3%"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/percentText"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/percentText"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="148dp"
        android:background="@drawable/textborder"
        android:gravity="center"
        android:text="최저가의 3%"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/SumIngredientText"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="148dp"
        android:background="@drawable/textborder"
        android:gravity="center"
        android:text="재료 총합"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/percentText" />

</androidx.constraintlayout.widget.ConstraintLayout>